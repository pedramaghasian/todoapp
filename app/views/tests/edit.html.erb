<h1> This Edit Page for Test <h1>

<% if @test.errors.any? %>
	<ul>
		<h2> sole this Errors : <h2>
		<% @test.errors.full_messages.each do |m|%>
			<li><%=m%></li>
		<%end%>
	</ul>
<%end%>


<%= form_for @test do |f|%>
	<p>
		<%= f.label :name %> <br/>
		<%= f.text_field :name%>
	</p>

	<p>
		<%= f.label :family%> <br/>
		<%= f.text_area :family%>
	</p>

	<p>
		<%= f.submit%>
	</p>

<%end%>

